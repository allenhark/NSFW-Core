{
  "typescript": true,
  "commands": [
    "./commands",
    "@adonisjs/core/build/commands/index.js",
    "@adonisjs/repl/build/commands",
    "@adonisjs/lucid/build/commands",
    "adonis5-scheduler/build/commands"
  ],
  "exceptionHandlerNamespace": "App/Exceptions/Handler",
  "aliases": {
    "App": "app",
    "Config": "config",
    "Database": "database",
    "Contracts": "contracts"
  },
  "preloads": [
    "./start/routes",
    "./start/kernel",
    "./start/events",
    {
      "file": "./start/wsKernel",
      "environment": [
        "web",
        "test"
      ],
      "optional": true
    },
    {
      "file": "./start/algo",
      "environment": [
        "web",
        "test"
      ],
      "optional": true
    },
    {
      "file": "./start/socket",
      "environment": [
        "web",
        "test"
      ],
      "optional": true
    }
  ],
  "providers": [
    "./providers/AppProvider",
    "@adonisjs/core",
    "@adonisjs/lucid",
    "@adonisjs/auth",
    "@adonisjs/redis",
    "@ruby184/adonis-socket.io",
    "adonis5-scheduler",
    "@adonisjs/attachment-lite",
    "@adonisjs/drive-s3",
    "adonis-bumblebee-ts",
    "adonis-responsive-attachment"
  ],
  "aceProviders": [
    "@adonisjs/repl"
  ],
  "tests": {
    "suites": [
      {
        "name": "functional",
        "files": [
          "tests/functional/**/*.spec(.ts|.js)"
        ],
        "timeout": 60000
      }
    ]
  },
  "testProviders": [
    "@japa/preset-adonis/TestsProvider"
  ],
  "wsExceptionHandlerNamespace": "App/Exceptions/WsHandler"
}